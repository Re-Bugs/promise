<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 배경색을 흰색으로 설정 -->
    <solid android:color="@android:color/white" />

    <!-- 테두리를 검정색, 2dp 너비로 설정 -->
    <stroke
        android:width="2dp"
        android:color="@android:color/black" />

    <!-- 테두리 모서리를 둥글게 처리하고 싶다면 radius 속성 추가 (선택 사항) -->
    <corners android:radius="4dp" />
</shape>